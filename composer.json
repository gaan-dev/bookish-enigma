{
    "require": {
        "php": "^7.3",
        "bramus/router": "~1.4",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/collections": "^8.8",
        "illuminate/database": "^8.8",
        "vlucas/phpdotenv": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["bootstrap/functions.php"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
