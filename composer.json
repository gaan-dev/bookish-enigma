{
    "require": {
        "php": "^7.3",
        "illuminate/database": "^8.8",
        "vlucas/phpdotenv": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
